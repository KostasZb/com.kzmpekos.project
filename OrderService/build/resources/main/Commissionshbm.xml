<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kzmpekos.commissions.Commission" table="commission" schema="commission">
        <id name="orderId">
            <column name="orderId" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="UserId">
            <column name="UserId" sql-type="int"/>
        </property>
        <property name="farmerId">
            <column name="farmer_id" sql-type="int"/>
        </property>
        <property name="timestamp">
            <column name="timestamp" sql-type="TIMESTAMP"/>
        </property>
        <property name="TotalCost">
            <column name="TotalCost" sql-type="float" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>